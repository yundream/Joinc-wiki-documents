&#47564;&#50557; &#54532;&#47196;&#44536;&#47016;&#51060; &#54540;&#47084;&#44536;&#51064;&#44284; &#45796;&#51060;&#45208;&#48121;&#54616;&#44172; &#47553;&#53356;&#46104;&#45716; &#54805;&#53468;&#51060;&#51648;&#47564;, &#54532;&#47196;&#44536;&#47016;&#51060; &#54532;&#47084;&#44536;&#51064;&#51032; &#47700;&#51064; &#54632;&#49688;&#47484; &#47751;&#44032;&#51648; &#50741;&#49496;&#44284; &#54632;&#44760; &#54840;&#52636;&#54616;&#44256; &#50668;&#44592;&#50640; &#45824;&#54620; &#47532;&#53556;&#44050;&#51012; &#44592;&#45796;&#47532;&#45716; &#54805;&#53468;&#47196; &#47564;&#46308;&#50612; &#51276;&#45796;&#47732;, &#47588;&#50864; &#54032;&#45800;&#54616;&#44592; &#50612;&#47140;&#50868; &#44221;&#50864;&#44032; &#46121;&#45768;&#45796;.
