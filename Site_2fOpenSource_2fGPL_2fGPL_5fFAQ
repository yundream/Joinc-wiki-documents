GPL&#51008; &#54532;&#47196;&#44536;&#47016;&#51012; &#50948;&#54644;&#49436; &#49444;&#44228;&#46108; &#44163;&#51077;&#45768;&#45796;. &#46384;&#46972;&#49436; GPL&#50640;&#45716; &#47588;&#45684;&#50620;&#51060;&#45208; &#49436;&#51201;&#50640;&#45716; &#54644;&#45817;&#46104;&#51648; &#50506;&#44256; &#54532;&#47196;&#44536;&#47016;&#50640;&#47564; &#51201;&#50857;&#46112; &#49688; &#51080;&#45716; &#47566;&#51008; &#51312;&#54637;&#46308;&#51060; &#54252;&#54632;&#46104;&#50612; &#51080;&#49845;&#45768;&#45796;. &#51060;&#50640; &#48152;&#54644;&#49436; GFDL&#50640;&#45716; &#51088;&#50976; &#47588;&#45684;&#50620;&#51032; &#48156;&#54665;&#51088;&#46308;&#50640;&#44172; &#51060;&#51061;&#51012; &#51452;&#44592; &#50948;&#54620; &#51312;&#54637;&#46308;&#51060; &#54252;&#54632;&#46104;&#50612; &#51080;&#49845;&#45768;&#45796;.
