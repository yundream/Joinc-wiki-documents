&#47564;&#50557; &#51060;&#47084;&#54620; &#47928;&#44396;&#44032; &#45572;&#46973;&#46108;&#45796;&#47732;, &#44060;&#51221;&#46112; GPL&#51032; &#45236;&#50857;&#50640; &#45824;&#54644;&#49436; &#51060;&#48120; &#48176;&#54252;&#46104;&#50612; &#51080;&#45716; GPL &#49548;&#54532;&#53944;&#50920;&#50612;&#51032; &#49688;&#47566;&#51008; &#51200;&#51089;&#44428;&#51088;&#46308;&#44284; &#50724;&#47019;&#46041;&#50504; &#54801;&#51032; &#44284;&#51221;&#51012; &#44144;&#52432;&#50556; &#54616;&#45716;&#45936;, &#51060;&#44163;&#51008; &#44144;&#51032; &#48520;&#44032;&#45733;&#54620; &#51068;&#51077;&#45768;&#45796;. &#49892;&#51228;&#51201;&#51064; &#44288;&#51216;&#50640;&#49436; &#48380; &#46412;, &#51060;&#44163;&#51008; GNU &#49548;&#54532;&#53944;&#50920;&#50612;&#50640; &#45824;&#54620; &#48176;&#54252; &#44508;&#51221;&#51012; &#53685;&#51068;&#51201;&#51004;&#47196; &#51312;&#51221;&#54616;&#45716; &#44163;&#51060; &#48520;&#44032;&#45733;&#54616;&#44172; &#46108;&#45796;&#45716; &#44163;&#51012; &#51032;&#48120;&#54633;&#45768;&#45796;.
