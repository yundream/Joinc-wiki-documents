&#47932;&#47200;, &#47784;&#46304; &#44592;&#50668;&#51088;&#46308;&#51060; &#44536;&#46308;&#51032; &#54532;&#47196;&#44536;&#47016;&#51012; &#44277;&#44060; &#54532;&#47196;&#44536;&#47016;&#51004;&#47196; &#47564;&#46308;&#50612;&#49436; &#51200;&#51089;&#44428;&#51012; &#54252;&#44592;&#54620;&#45796;&#47732; GPL&#51012; &#44053;&#51228;&#54624; &#49688; &#51080;&#45716; &#51200;&#51089;&#44428; &#51088;&#52404;&#44032; &#50630;&#50612;&#51648;&#45716; &#44163;&#44284; &#44057;&#49845;&#45768;&#45796;. &#44536;&#47000;&#49436; &#50864;&#47532;&#45716; &#47566;&#51008; &#50577;&#51032; &#53076;&#46300;&#50640; &#45824;&#54644;&#49436;&#45716; &#51200;&#51089;&#44428;&#51012; &#49444;&#51221;&#54616;&#44256;, &#50724;&#51649; &#51687;&#51008; &#49688;&#51221;&#50640; &#45824;&#54620; &#48512;&#48516;&#47564;&#51012; &#44277;&#44060; &#54532;&#47196;&#44536;&#47016;&#51004;&#47196; &#47564;&#46308;&#46020;&#47197; &#49324;&#46988;&#46308;&#50640;&#44172; &#47568;&#54616;&#44256; &#51080;&#49845;&#45768;&#45796;.
