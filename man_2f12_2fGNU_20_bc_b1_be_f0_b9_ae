&#49888;&#45392;&#51012; &#54252;&#44592;&#54616;&#51648; &#50506;&#44256;&#46020; &#52980;&#54504;&#53552;&#47484; &#44228;&#49549; &#49324;&#50857;&#54616;&#44592; &#50948;&#54644;&#49436; &#45208;&#45716; &#49324;&#50857;&#51060; &#51228;&#54620;&#46104;&#45716; &#49548;&#54532;&#53944;&#50920;&#50612; &#50630;&#51060;&#46020; &#51089;&#50629;&#51012; &#54644; &#45208;&#44040; &#49688; &#51080;&#45716; &#52649;&#48516;&#54620; &#51088;&#50976; &#49548;&#54532;&#53944;&#50920;&#50612;&#51032; &#48376;&#52404;&#47484; &#47564;&#46308; &#44208;&#49900;&#51012; &#54664;&#45796;. &#45208;&#45716; MIT&#52769;&#51060; &#50612;&#46496;&#54620; &#48277;&#47456;&#51201; &#44540;&#44144;&#50640; &#51032;&#54644;&#49436;&#46020; GNU&#51032; &#51088;&#50976;&#47196;&#50868; &#48176;&#54252;&#47484; &#51228;&#51648;&#54616;&#51648; &#47803;&#54616;&#46020;&#47197; &#54616;&#44592; &#50948;&#54644;&#49436; &#51064;&#44277;&#51648;&#45733; &#50672;&#44396;&#49548;&#47484; &#44536;&#47564;&#46160;&#50632;&#45796;.
