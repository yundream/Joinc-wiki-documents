&#54616;&#51648;&#47564;, &#47564;&#50557; &#54532;&#47196;&#44536;&#47016;&#51060; &#54540;&#47084;&#44536;&#51064;&#44284; &#45796;&#51060;&#45208;&#48121;&#54616;&#44172; &#47553;&#53356;&#46104;&#45716; &#54805;&#49885;&#51004;&#47196; &#49892;&#54665;&#46104;&#50612; &#49436;&#47196; &#54632;&#49688;&#47484; &#54840;&#52636;&#54616;&#44256; &#51088;&#47308; &#44396;&#51312;&#47484; &#44277;&#50976;&#54616;&#44172; &#46108;&#45796;&#47732; &#51060;&#45716; &#54616;&#45208;&#51032; &#54532;&#47196;&#44536;&#47016;&#51012; &#54805;&#49457;&#54616;&#44172; &#46108;&#45796;&#44256; &#48380; &#49688; &#51080;&#51004;&#48064;&#47196; &#54540;&#47084;&#44536;&#51064;&#51008; &#47700;&#51064; &#54532;&#47196;&#44536;&#47016;&#51060; &#54869;&#51109;&#46108; &#44163;&#51004;&#47196; &#44036;&#51452;&#46104;&#50612;&#50556; &#54633;&#45768;&#45796;. &#51060;&#44163;&#51008; &#54540;&#47084;&#44536;&#51064;&#50640;&#46020; GPL&#51060;&#45208; GPL&#44284; &#54840;&#54872;&#46104;&#45716; &#46972;&#51060;&#49440;&#49828;&#47484; &#51201;&#50857;&#54644;&#50556; &#54620;&#45796;&#45716; &#44163;&#51012; &#51032;&#48120;&#54633;&#45768;&#45796;.
